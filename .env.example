# API Keys
OPENAI_API_KEY=your-openai-api-key-here
LANGSMITH_API_KEY=your-langsmith-api-key-here
TOGETHER_API_KEY=your-together-api-key-here

# LangSmith Configuration
LANGSMITH_TRACING=true
LANGSMITH_PROJECT=multi-agent-system

# Model Configuration
MODEL_NAME=gpt-4o-mini
TEMPERATURE=0.0

# Embedding Configuration
EMBEDDING_MODEL=Alibaba-NLP/gte-modernbert-base

# Database Configuration
DATABASE_URL=sqlite:///./app.db

# Memory Configuration
MEMORY_STORE_TYPE=memory  # Options: memory, redis, postgres

# Redis Configuration (if using Redis memory store)
# REDIS_URL=redis://localhost:6379/0
# REDIS_PASSWORD=your-redis-password

# PostgreSQL Configuration (if using PostgreSQL memory store)
# POSTGRES_HOST=localhost
# POSTGRES_PORT=5432
# POSTGRES_DB=multiagent
# POSTGRES_USER=your-username
# POSTGRES_PASSWORD=your-password

# Application Configuration
DEBUG=false
LOG_LEVEL=INFO 